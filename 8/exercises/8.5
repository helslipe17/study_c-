#include <iostream>
using namespace std;
const int SIZE = 5;

template <typename Any>
Any max5(Any * arr);


int main(void){
    int arr1[SIZE] = {1, 3, 5, 7, 9};
    double arr2[SIZE] = {2.3, 5.7, 1.9, 9.2, 4.5};
    cout << "Max number of arr1 = " << max5(arr1) << endl;
    cout << "Max number of arr2 = " << max5(arr2) << endl;
    cout << endl;
    return 0;
}
template <typename Any>
Any max5(Any * arr){
    Any maxValue = arr[0];
    for(int i = 0; i < SIZE; i++){
        if(arr[i] > maxValue)
            maxValue = arr[i];
    }
    return maxValue;
}

