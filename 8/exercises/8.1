#include <iostream>
#include <string>
using namespace std;

void Show(string str);
void Show(string  str, int a);
int main(void){
    int count = 2;
    string hel = "Hello";
    string wor = "World!";
    Show(hel);
    Show(hel);
    Show(wor, count);
   
    
    cout << endl;
    return 0;
}

void Show(string str){
    cout << str << endl;
}

void Show(string  str, const int a){
    if(a > 0){
        for(int i = 0; i < a; i++)
            cout <<  str << endl;
    }
    else{
        cout << str << endl;
    }
}
