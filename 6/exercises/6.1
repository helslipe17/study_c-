#include <iostream>
#include <cctype>

int main(){
    using namespace std;
    
    char ch;
    cin.get(ch);
    while ( ch != '@'){
        if(isalpha(ch)){
            if(islower(ch)){
                ch = toupper(ch);
                cout << ch;
            }
            else{
                ch = tolower(ch);
                cout << ch;
            }
        }else
            cout << ch;
        cin.get(ch);
    }
    
    cout << endl;
    
    return 0;
}
